{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_SITE_URL": "https://missstarinternational-website.vercel.app",
    "NEXT_PUBLIC_SITE_NAME": "Miss Star International"
  },
  "github": {
    "enabled": true,
    "silent": true
  }
} 